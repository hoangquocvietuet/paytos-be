  "engines": {
    "node": ">=20.11.1",
    "npm": ">=10.1.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.mjs",
    "test:watch": "jest --config jest.config.mjs --watch",
    "test:cov": "jest --config jest.config.mjs --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.mjs --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --config jest.config.mjs --testPathPattern=spec.ts",
    "test:unit:watch": "jest --config jest.config.mjs --testPathPattern=spec.ts --watch",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:generate src/migrations/migrations",
    "migration:up": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:run",
    "migration:down": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:revert"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^3.1.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
